<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:BookingApp"
                    xmlns:v="clr-namespace:BookingApp.Views"
                    xmlns:vm="clr-namespace:BookingApp.ViewModels">
    <DataTemplate DataType="{x:Type vm:StartPageViewModel}">
        <v:StartPage/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:SignUpViewModel}">
        <v:SignUpView/>
    </DataTemplate>
</ResourceDictionary>